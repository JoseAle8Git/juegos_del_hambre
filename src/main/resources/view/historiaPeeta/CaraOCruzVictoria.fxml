<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="juego.historiaPeeta.controladores.ControllerCaraOCruzVictoria"
           stylesheets="/css/cssPeeta/caraOCruzVictoria.css">
    <ImageView styleClass="imagen">
        <image>
            <Image url="/images/imagesPeeta/distrito12.png" requestedHeight="720" requestedWidth="1221"/>
        </image>
    </ImageView>
    <HBox fx:id="caja">
        <ImageView fitHeight="300" preserveRatio="true">
            <Image url="/images/imagesPeeta/guardiaConCasco.png"/>
        </ImageView>
        <VBox styleClass="vbox">
            <Label fx:id="texto" styleClass="texto"/>
            <Button text="Continuar" onAction="#continuar"/>
        </VBox>
    </HBox>
</StackPane>