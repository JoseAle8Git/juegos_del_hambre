<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="juego.historiaEliot.controladores.cap2.dispensario.ControllerHistoriaEliotCap2_Dispensario1"
            prefHeight="720" prefWidth="1221"
            fx:id="panelAP"
            stylesheets="/css/historiaEliotCap2_Dispensario1.css">

    <ImageView
            AnchorPane.bottomAnchor="60"
            AnchorPane.rightAnchor="30"
            fitHeight="350"
            fitWidth="350">
        <image>
            <Image url="/images/personajeLuna.png" />
        </image>
    </ImageView>
    <Label fx:id="fraseLabel" AnchorPane.bottomAnchor="60" AnchorPane.leftAnchor="60" alignment="CENTER">
        <font>
            <Font name="System Bold" size="20" />
        </font>
    </Label>
    <VBox alignment="CENTER" AnchorPane.bottomAnchor="210" AnchorPane.leftAnchor="300" spacing="30">
        <Label text="Contestar:" styleClass="label-claro" />
        <HBox alignment="CENTER" spacing="60">
            <Button fx:id="opcionA" onAction="#irAOpcionA" />
            <Button fx:id="opcionB" onAction="#irAOpcionB" />
        </HBox>
    </VBox>

</AnchorPane>
