<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="juego.historiaEliot.controladores.cap2.ControllerHistoriaEliotCap2_EleccionCamino"
            prefHeight="720" prefWidth="1221"
            fx:id="panelVBox"
            stylesheets="/css/eleccionCamino.css">

    <Label text="Selecciona un camino:" styleClass="label-titulo" textAlignment="CENTER">
        <font>
            <Font name="System Bold" size="55" />
        </font>
    </Label>
    <HBox alignment="CENTER" spacing="50">
        <Button text="El Claro" fx:id="btnClaro" onAction="#irAElClaro" />
        <Button text="El Dispensario" fx:id="btnDispensario" onAction="#irAElDispensario" />
        <Button text="El Bosque" fx:id="btnBosque" onAction="#irAElBosque" />
    </HBox>

</VBox>
