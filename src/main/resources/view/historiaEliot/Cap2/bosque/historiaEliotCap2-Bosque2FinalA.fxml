<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="juego.historiaEliot.controladores.cap2.bosque.ControllerHistoriaEliotCap2_Bosque2FinalA"
            prefHeight="720" prefWidth="1221"
            fx:id="panelAP"
            stylesheets="/css/cap2Final.css">

    <ImageView
            AnchorPane.bottomAnchor="60"
            AnchorPane.rightAnchor="30"
            fitHeight="350"
            fitWidth="350">
        <image>
            <Image url="/images/personajeGalen.png" />
        </image>
    </ImageView>
    <ImageView
            AnchorPane.bottomAnchor="60"
            AnchorPane.leftAnchor="30"
            fitHeight="350"
            fitWidth="350">
        <image>
            <Image url="/images/eliotPersonajeInicio.png" />
        </image>
    </ImageView>
    <Label fx:id="fraseLabel" wrapText="true" AnchorPane.topAnchor="150">
        <font>
            <Font name="System Bold" size="36" />
        </font>
    </Label>

</AnchorPane>
