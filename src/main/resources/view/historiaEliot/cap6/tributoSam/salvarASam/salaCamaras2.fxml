<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="juego.historiaEliot.controladores.cap6.tributoSam.salvarASam.ControllerSalaCamaras2"
            prefHeight="720" prefWidth="1221"
            fx:id="panelAP"
            stylesheets="/css/salaCamaras2.css">

